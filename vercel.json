{
  "buildCommand": "npm run build -- --configuration production --source-map=false",
  "outputDirectory": "dist/fullstackproject",
  "installCommand": "npm install",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

